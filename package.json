  "bugs": {
    "url": "https://github.com/jonly03/goggins-shop/issues"
  },
  "homepage": "https://github.com/jonly03/goggins-shop#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
